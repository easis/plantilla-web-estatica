{
  "name": "plantilla-web-estatica",
  "version": "1.0.0",
  "description": "Plantilla para generar páginas web estáticas.",
  "main": ".eleventy.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s clean build:11ty",
    "build:11ty": "cross-env NODE_ENV=production eleventy",
    "dev": "cross-env NODE_ENV=development run-s clean dev:11ty",
    "dev:11ty": "cross-env NODE_ENV=development eleventy",
    "dev:verbose": "cross-env NODE_ENV=development DEBUG=* run-s dev",
    "serve": "eleventy --serve",
    "clean": "rimraf dist _site",
    "test": "echo \"test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://easis@github.com/easis/plantilla-web-estatica.git"
  },
  "keywords": [
    "plantilla",
    "web",
    "estatica",
    "html"
  ],
  "author": "Erik Asís",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/easis/plantilla-web-estatica/issues"
  },
  "homepage": "https://github.com/easis/plantilla-web-estatica#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.9.0",
    "autoprefixer": "^10.2.5",
    "chalk": "^4.1.1",
    "cross-env": "^7.0.3",
    "eleventy-plugin-babel": "^1.0.1",
    "eleventy-plugin-sass": "^1.2.0",
    "modern-normalize": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.1.2"
  },
  "dependencies": {}
}
